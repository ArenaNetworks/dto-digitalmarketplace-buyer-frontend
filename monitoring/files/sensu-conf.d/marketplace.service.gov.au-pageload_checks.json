{
  "checks": {
    "marketplace.service.gov.au-homepage_load": {
      "handlers": ["default", "slack", "pagerduty"],
      "command": "/opt/sensu/embedded/bin/check-http.rb --timeout 10 --url ${URL} --response-code 200 --query '<title>[^<]*[Mm]arketplace[^<]*</title>'",
      "interval": 300,
      "standalone": true
    },
    "marketplace.service.gov.au-catalogue_load": {
      "handlers": ["default", "slack", "pagerduty"],
      "command": "/opt/sensu/embedded/bin/check-http.rb --timeout 30 --url ${URL}search/sellers --response-code 200 --query '[sS]ellers'",
      "interval": 300,
      "standalone": true
    }
  }
}
